{
    "manifestVersion": 1,
    "id": "calculate-extra-azure-devops-variables",
    "name": "Calculate extra Azure DevOps variables",
    "version": "1.0.0",
    "public": true,
    "publisher": "FlorisDevreese",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Pipeline task that calculates extra Azure DevOps related environment variables.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "variables",
        "sprint",
        "iteration"
    ],
    "links": {
        "support": { "uri": "https://github.com/FlorisDevreese/Azure-DevOps-Extensions/issues" }
    },
    "icons": {
        "default": "images/extension-icon.png",
        "large": "images/extension-icon-large.png"
    },
    "screenshots": [
        { "path": "images/classic-build-screenshot.png" }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/FlorisDevreese/Azure-DevOps-Extensions"
    },
    "content": {
        "details": { "path": "README.md" }
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}